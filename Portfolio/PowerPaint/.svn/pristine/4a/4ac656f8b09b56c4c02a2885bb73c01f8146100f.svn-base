 */
public abstract class AbstractTool implements Tool {

    private final char myLetter;
    
    /**
     * 
     */
    public AbstractTool(final char theLetter) {
        myLetter = theLetter;
    }

    public char getLetter() {
        return myLetter;
    }
}
